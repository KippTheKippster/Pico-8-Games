function _update60()

	dir = 4
	
	if(btn(0)) 
			then x=x-(1) dir = 1
			
	elseif(btn(1)) 
			then x=x+1 dir = 0 end
			
	if(btnp(2)) then y=y-1 ass = -3.5 dir = 2
			
 elseif(btn(3)) 
 		then y=y+1 dir = 3 end
	
	ypsd = yspd + 4
	if(y < 64) then
	 ass = ass + 0.1
		y=y+ass
	else
 y = min(y,64)
	end
	
end
	
	
